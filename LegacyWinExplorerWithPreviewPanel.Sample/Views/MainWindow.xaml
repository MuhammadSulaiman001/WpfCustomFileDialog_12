<Window x:Class="LegacyWinExplorerWithPreviewPanel.Sample.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        mvvm:ViewModelLocator.AutoWireViewModel="True"
        xmlns:mvvm="http://prismlibrary.com/"
        xmlns:viewModels="clr-namespace:LegacyWinExplorerWithPreviewPanel.Sample.ViewModels"
        d:DataContext="{d:DesignInstance viewModels:MainWindowViewModel}"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Open Old Windows Dialog"
                Command="{Binding OpenSampleDialogCommand}"
                Padding="8"
                HorizontalAlignment="Center"
                VerticalAlignment="Center" />
    </Grid>
</Window>